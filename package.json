{
  "name": "sputnik-v2-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start-api": "NEST_APP_TYPE=api nest start",
    "start-api:dev": "NEST_APP_TYPE=api nest start --watch",
    "start-aggregator": "NEST_APP_TYPE=aggregator nest start --config aggregator-nest-cli.json",
    "start-aggregator:dev": "NEST_APP_TYPE=aggregator nest start --watch --config aggregator-nest-cli.json",
    "start-aggregator-gross": "NEST_APP_TYPE=aggregator-gross nest start --config aggregator-gross-nest-cli.json",
    "start-aggregator-gross:dev": "NEST_APP_TYPE=aggregator-gross nest start --watch --config aggregator-gross-nest-cli.json",
    "start-notifier": "NEST_APP_TYPE=notifier nest start --config notifier-nest-cli.json",
    "start-notifier:dev": "NEST_APP_TYPE=notifier nest start --watch --config notifier-nest-cli.json",
    "start": "concurrently \"yarn start-aggregator-gross\" \"yarn start-aggregator\" \"yarn start-api\" \"yarn start-notifier\"",
    "start:dev": "concurrently \"yarn start-aggregator-gross:dev\" \"yarn start-aggregator:dev\" \"yarn start-api:dev\" \"yarn start-notifier:dev\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-request": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/microservices": "^7.6.18",
    "@nestjs/websockets": "^7.6.18",
    "@nestjs/platform-socket.io": "^7.6.18",
    "@supercharge/promise-pool": "^1.7.0",
    "@types/cache-manager": "^3.4.2",
    "amqplib": "^0.8.0",
    "amqp-connection-manager": "^3.2.2",
    "aws-sdk": "^2.937.0",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "camelcase-keys": "^7.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "firebase-admin": "^9.10.0",
    "ioredis": "^4.14.1",
    "near-api-js": "^0.42.0",
    "object-hash": "^2.2.0",
    "parse-redis-url-simple": "^1.0.2",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "socket.io": "^4.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typeorm-naming-strategies": "^2.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/ioredis": "^4.27.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
